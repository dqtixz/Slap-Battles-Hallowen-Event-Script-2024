local Safe = false

MainSection:CreateToggle("Auto Farm CandyCorns", function(state)
    _G.Farming = state

    if _G.Farming then
        spawn(function() 
            while _G.Farming do
                local folder = game.Workspace:FindFirstChild("CandyCorns") 
                if folder and #folder:GetChildren() > 0 then
                    for _, part in pairs(folder:GetChildren()) do
                        if part:IsA("BasePart") then 
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part.CFrame
                            wait(_G.cdtp2)
                        end
                    end
                else
                    local safe = true
                    if safe == true then
                        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(85, -75, -87)))
                    end
                end
                wait(_G.cdtp)
            end
        end)
    end
end)

MainSection:CreateButton("Esp CandyCorns/Candy RGB", function()

    local function Colorido()
        local time = tick() % 5
        local hue = time / 5
        return Color3.fromHSV(hue, 1, 1)
    end

    local function applyESP(object)
        if not object:FindFirstChildOfClass("Highlight") then
            local highlight = Instance.new("Highlight")
            highlight.Parent = object
            highlight.FillTransparency = 0.5
            highlight.OutlineTransparency = 0
        end
    end

    local function neymar()
        while true do
            local folder = game.Workspace:FindFirstChild("CandyCorns")
            if folder then
                for _, object in pairs(folder:GetChildren()) do
                    if object:IsA("BasePart") then
                        applyESP(object)
                        local highlight = object:FindFirstChildOfClass("Highlight")
                        if highlight then
                            highlight.FillColor = Colorido()
                            highlight.OutlineColor = Colorido()
                        end
                    end
                end
            end
            wait(0.1) 
        end
    end
    spawn(neymar)
end)

